\n    let utils = {\n        debounce,\n        throttle,\n    };\n\n    // 支持多库共存\n    let _$ = window.$;\n    utils.noConflict = function noConflict() {\n        if (window.$ === utils) {\n            window.$ = _$;\n        }\n        return utils;\n    };\n\n    //暴露API，支持：浏览器直接导入，webpack编译，node环境\n    if (typeof window !== \"undefined\") {\n        window.utils = utils;\n    }\n    if (typeof module === \"object\" && typeof module.exports === \"object\") {\n        module.exports = utils;\n    }\n})();\n"]}]}